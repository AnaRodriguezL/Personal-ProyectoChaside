<!doctype html>
<html>

<head>
	<meta charset="utf-8" />
	<link rel="icon" type="image/png" href="assets/img/logoUcp/favicon.ico" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

	<title>CHASIDE</title>

	<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
	<meta name="viewport" content="width=device-width" />

	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css"
		integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">

	<!-- Bootstrap CSS -->
	<link rel="stylesheet" type="text/css" href="librerias/bootstrap/css/bootstrap.css">

	<!-- Animate CSS -->
	<link rel="stylesheet" type="text/css" href="librerias/animate/animate.css">

	<!-- Sweet-alert CSS -->
	<link rel="stylesheet" type="text/css" href="librerias/sweet-alert/sweetalert.css">

	<!-- Material Design Icons CSS -->
	<link rel="stylesheet" type="text/css" href="librerias/material-design-icons/materialdesignicons.min.css">

	<!-- Bundle css -->
	<link rel="stylesheet" type="text/css" href="librerias/bundle/bundle.base.css">

	<!-- Datatable css -->
	<link rel="stylesheet" type="text/css" href="librerias/DataTables/datatables.min.css" />

	<!-- Bootstrap Select css -->
	<link rel="stylesheet" type="text/css" href="librerias/bootstrap-select/dist/css/bootstrap-select.min.css" />

	<!-- Summer note css -->
	<link rel="stylesheet" type="text/css" href="librerias/summernote/summernote-lite.css" />

	<!-- Loader css -->
	<link rel="stylesheet" type="text/css" href="assets/css/loader.css" />

	<!-- CSS FILES -->
	<link rel="stylesheet" type="text/css" href="assets/css/login.css">
	<link rel="stylesheet" type="text/css" href="assets/css/buttons.css">
	<link rel="stylesheet" type="text/css" href="assets/css/adminTemplate.css">

	<!-- CSS vista -->
	{{{cssFile}}}
</head>

<body>
	{{!-- LOADER --}}
	<div id="loader" class="active">
		<div class="loader18 d-flex align-items-center">Loading...</div>
	</div>
	<div class="container-scroller">

		{{!-- BARRA PARTE SUPERIOR --}}
		<nav class="navbar default-layout-navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row"
			style="background-color:#BB2C2C;">
			<div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-center"
				style="background-color:#BB2C2C;">
				<a class="navbar-brand brand-logo" href="/adminRoute"><img src="assets/img/logoUcp/logoBlanco.png"
						alt="logo" /></a>
				<a class="navbar-brand brand-logo-mini" href="/adminRoute"><img src="assets/img/logoUcp/logoBlanco.png"
						alt="logo" /></a>
			</div>
			<div class="navbar-menu-wrapper d-flex align-items-stretch">

				{{!-- ICON MENU D-NORMAL --}}
				<ul class="navbar-nav navbar-nav-left">
					<button class="navbar-toggler navbar-toggler-left align-self-center" type="button"
						data-toggle="offcanvas">
						<span class="mdi text-white mdi-menu"></span>
					</button>
				</ul>
				<ul class="navbar-nav navbar-nav-right">
					<li class="nav-item nav-profile dropdown">

						{{!-- PERFIL USUARIO --}}
						<a class="nav-link dropdown-toggle" id="profileDropdown" href="#" data-toggle="dropdown"
							aria-expanded="false">
							<div class="nav-profile-img">
								<img src="assets/img/imagenesUsuario/icono-usuario.png" alt="image">
								<span class="availability-status online"></span>
							</div>
							<div class="nav-profile-text">
								<p class="mb-1 text-black">{{nombreCompleto}}</p>
							</div>
						</a>
						<div class="dropdown-menu navbar-dropdown" aria-labelledby="profileDropdown">

							{{!-- EDITAR PERFIL --}}
							<a class="dropdown-item d-none" href="/editarPerfilRoute">
								<i class="mdi mdi-pencil mr-2 text-success"></i>
								Editar perfil
							</a>

							{{!-- CERRAS SESIÓN --}}
							<div class="dropdown-divider d-none"></div>
							<a class="dropdown-item" href="/cerrarSesionRoute">
								<i class="mdi mdi-logout mr-2 text-info"></i>
								Cerrar sesión
							</a>
						</div>
					</li>

					{{!-- ICON EXTENDER PANTALLA --}}
					<li class="nav-item d-none d-lg-block full-screen-link">
						<a class="nav-link">
							<i class="mdi text-white mdi-fullscreen" id="fullscreen-button"></i>
						</a>
					</li>
				</ul>

				{{!-- ICON MENU D-LG --}}
				<button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button"
					data-toggle="offcanvas">
					<span class="mdi text-white mdi-menu"></span>
				</button>
			</div>
		</nav>

		{{!-- SIDENAV --}}
		<div class="container-fluid page-body-wrapper">
			<nav class="sidebar sidebar-offcanvas" id="sidebar">
				<ul class="nav">

					{{!-- PERFIL USUARIO --}}
					<li class="nav-item nav-profile">
						<a href="#" class="nav-link">
							<div class="nav-profile-image">
								<img src="assets/img/imagenesUsuario/icono-usuario.png" alt="profile">
								<span class="login-status online"></span>
							</div>
							<div class="nav-profile-text d-flex flex-column">
								<span class="font-weight-bold mb-2">{{nombreCompleto}}</span>
								<span class="text-secondary text-small">Estudiante</span>
							</div>
						</a>
					</li>

					{{!-- SECCIÓN REALIZAR PRUEBA --}}
					<li class="nav-item active" id="btnPrueba">
						<a class="nav-link" href="/pruebaRoute">
							<span class="menu-title">Realizar Prueba</span>
							<i class="mdi mdi-clipboard-text menu-icon"></i>
						</a>
					</li>

					{{!-- SECCIÓN CARRERAS (FACULTADES) --}}
					<li class="nav-item" id="btnFacultades">
						<a class="nav-link" href="/facultadesRoute">
							<span class="menu-title">Oferta Académica</span>
							<i class="mdi mdi-school menu-icon"></i>
						</a>
					</li>

					{{!-- SECCIÓN MIS RESULTADOS --}}
					<li class="nav-item" id="btnResultados">
						<a class="nav-link" href="/resultadosRoute">
							<span class="menu-title">Mis Resultados</span>
							<i class="mdi mdi-book-open-page-variant menu-icon"></i>
						</a>
					</li>

					{{!-- SECCIÓN CONTACTENOS --}}
					<li class="nav-item" id="btnContactenos">
						<a class="nav-link" href="/contactenosRoute">
							<span class="menu-title">Contáctenos</span>
							<i class="mdi mdi-phone-in-talk menu-icon"></i>
						</a>
					</li>
				</ul>
			</nav>
			{{{body}}}
		</div>
	</div>
</body>

<!-- Jquery -->
<script src="librerias/jquery/jquery-3.4.1.min.js" type="text/javascript"></script>

<!-- Jquery Validation -->
<script src="librerias/jquery-validation/dist/jquery.validate.min.js" type="text/javascript"></script>

<!-- Tilt JS -->
<script src="librerias/tilt/tilt.jquery.min.js" type="text/javascript"></script>

<!-- Popper JS -->
<script src="librerias/bootstrap/js/popper.js" type="text/javascript"></script>

<!-- Bootstrap JS -->
<script src="librerias/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>

<!-- Sweet-alert CSS -->
<script src="librerias/sweet-alert/sweetalert.min.js" type="text/javascript"></script>

<!-- Perfect Scrollbar JS -->
<script src="librerias/perfect-scrollbar/perfect-scrollbar.js" type="text/javascript"></script>

<!-- Perfect Charts JS -->
<script src="librerias/charts/charts.js" type="text/javascript"></script>

<!-- Misc JS -->
<script src="librerias/misc/misc.js" type="text/javascript"></script>

<!-- Off Canvas JS -->
<script src="librerias/off-canvas/off-canvas.js" type="text/javascript"></script>

<!-- Query JS -->
<script src="librerias/Query/Query.js" type="text/javascript"></script>

<!-- Datatable JS -->
<script src="librerias/DataTables/datatables.min.js" type="text/javascript"></script>

<!-- Auto Numeric JS -->
<script src="librerias/autoNumeric/autoNumeric.min.js" type="text/javascript"></script>

<!-- Bootstrap Select JS -->
<script src="librerias/bootstrap-select/dist/js/bootstrap-select.min.js" type="text/javascript"></script>

<!-- Bootstrap Select JS -->
<script src="librerias/summernote/summernote-lite.js" type="text/javascript"></script>

<!-- JS para descargar Excel DT -->
<script src="https://cdn.datatables.net/buttons/1.5.6/js/dataTables.buttons.min.js" type="text/javascript"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js" type="text/javascript"></script>
<script src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.html5.min.js" type="text/javascript"></script>
<script src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.flash.min.js" type="text/javascript"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js" type="text/javascript"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js" type="text/javascript"></script>
<script src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.print.min.js" type="text/javascript"></script>

<!-- Validar campos -->
<script src="librerias/ValidarCampos.js" type="text/javascript"></script>

<!-- Sidebar JS -->
<script src="assets/js/sidebar.js" type="text/javascript"></script>

<!-- File upload JS -->
<script src="assets/js/file-upload.js" type="text/javascript"></script>

<!-- JS vista -->
{{{jsFile}}}

</html>